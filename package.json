  "scripts": {
    "config:nip05": "ts-node -P tsconfig.base.json set-env-nip05.ts",
    "config:nip05adm": "ts-node -P tsconfig.base.json set-env-nip05adm.ts",
    "start:nip05": "npm run config:nip05 && nx serve nip05",
    "start:nip05adm": "npm run config:nip05adm && nx serve nip05adm",
    "build:nip05": "npm run config:nip05 && nx build nip05",
    "build:delmo": "npm run config:delmo && nx build delmo",
    "build-dev:nip05": "npm run config:nip05 && nx build nip05 --configuration development",
    "build-dev:delmo": "npm run config:delmo && nx build delmo --configuration development"
  },
  "private": true,
  "custom": {
    "nip05": {
      "name": "nip05.social",
      "version": "2.1.2"
    },
    "nip05adm": {
